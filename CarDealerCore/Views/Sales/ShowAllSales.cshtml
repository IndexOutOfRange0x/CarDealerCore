@model IEnumerable<CarDealerCore.Models.Sale>
@{
    Layout = "/Views/Layout.cshtml";
}

<link rel="stylesheet" href="~/css/table.css" />
<table class="table_dark">
    <tr>
        <td>ID</td>
        <td>Buyer</td>
        <td>Car</td>>
        <td>Date of sale</td>>
        <td>Status</td>>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.UserId</td>
            <td>@item.CarId</td>
            <td>@item.Date_Sold</td>
            <td>@item.Status</td>

        </tr>
    }
</table>
